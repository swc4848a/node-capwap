  "bugs": {
    "url": "https://github.com/zqqiang/node-capwap/issues"
  },
  "homepage": "https://github.com/zqqiang/node-capwap#readme",
  "dependencies": {
    "debug": "^2.2.0",
    "express": "^4.13.3",
    "packet": "0.0.6",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-router": "^2.2.2",
    "serve-favicon": "^2.3.0",
    "stately.js": "fschaefer/Stately.js",
    "tap": "isaacs/node-tap",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
