{
  "name": "node-capwap",
  "version": "0.0.1",
  "description": "node-capwap ========= ### CAPWAP implementation in JavaScript (Work in progress)",
  "main": "app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch-css": "catw -c 'lessc -' 'public/theme/project/less/main.less' -o public/theme/project/css/main.css",
    "watch-js": "watchify public/main.js -o public/bundle.js -t [ babelify --presets [ es2015 react ] ] -dv",
    "watch": "npm run watch-js | npm run watch-css",
    "build-css": "catw -c 'lessc -' 'public/theme/project/less/main.less' > public/theme/project/css/main.css",
    "build-js": "browserify public/main.js > public/bundle.js",
    "build": "npm run build-css && npm run build-js",
    "start": "node monitor.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://zqqiang@github.com/zqqiang/node-capwap.git"
  },
